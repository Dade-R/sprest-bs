{
  "name": "gd-sprest-bs",
  "version": "1.1.5",
  "description": "SharePoint JavaScript, TypeScript and Web Components designed using the Bootstrap framework.",
  "main": "dist/gd-sprest-bs.js",
  "typings": "src/typings.d.ts",
  "scripts": {
    "all": "npm run build && npm run prod && npm run typings && npm run wc",
    "build": "webpack --mode=development",
    "prod": "webpack --mode=production",
    "typings": "dts-bundle --out ../dist/gd-sprest-bs.d.ts --name gd-sprest-bs --main ./src/typings.d.ts",
    "wc": "cd wc && npm run build",
    "wc-serv": "cd wc && npm run start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gunjandatta/sprest-bs.git"
  },
  "keywords": [
    "Bootstrap",
    "SharePoint",
    "Web Components"
  ],
  "author": "Gunjan Datta <me@dattabase.com> (https://gunjandatta.github.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gunjandatta/sprest-bs/issues"
  },
  "homepage": "https://gunjandatta.github.io/extras/bs",
  "dependencies": {
    "gd-bs": "^0.9.9",
    "gd-sprest": "^4.3.5",
    "gd-sprest-def": "^0.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@stencil/core": "^0.13.2",
    "babel-loader": "^8.0.4",
    "css-loader": "^1.0.0",
    "dts-bundle": "^0.7.3",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0",
    "stencil": "^0.0.5",
    "style-loader": "^0.22.1",
    "ts-loader": "^5.2.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2"
  }
}
