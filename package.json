{
  "name": "gd-sprest-bs",
  "version": "2.7.6",
  "description": "SharePoint JavaScript, TypeScript and Web Components designed using the Bootstrap framework.",
  "main": "dist/gd-sprest-bs-icons.js",
  "typings": "dist/gd-sprest-bs.d.ts",
  "scripts": {
    "all": "npm run build-all && npm run prod-all && npm run typings && npm run wc",
    "build-all": "npm run build && npm run build-icons",
    "build": "webpack --mode=development",
    "build-icons": "webpack --mode=development --icons=true",
    "prod-all": "npm run prod && npm run prod-icons",
    "prod": "webpack --mode=production",
    "prod-icons": "webpack --mode=production --icons=true",
    "typings": "dts-bundle --out ../dist/gd-sprest-bs.d.ts --name gd-sprest-bs --main ./@types/index.d.ts",
    "wc": "cd wc && npm run build",
    "wc-serve": "cd wc && npm run start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gunjandatta/sprest-bs.git"
  },
  "keywords": [
    "Bootstrap",
    "SharePoint",
    "Web Components"
  ],
  "author": "Gunjan Datta <me@dattabase.com> (https://dattabase.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gunjandatta/sprest-bs/issues"
  },
  "homepage": "https://dattabase.com/extras/bs",
  "dependencies": {
    "gd-bs": "^1.7.3",
    "gd-sprest": "^5.1.3",
    "gd-sprest-def": "^0.7.6"
  },
  "devDependencies": {
    "@babel/core": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@stencil/core": "^0.18.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "dts-bundle": "^0.7.3",
    "node-sass": "^4.13.0",
    "sass-loader": "^8.0.0",
    "stencil": "^0.0.5",
    "style-loader": "^1.0.0",
    "ts-loader": "^6.2.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  }
}
