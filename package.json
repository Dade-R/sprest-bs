    "gd-bs": "^0.4.5",
    "gd-sprest": "^4.2.1"
  },
  "devDependencies": {
    "@stencil/core": "^0.12.4",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^1.0.0",
    "dts-bundle": "^0.7.3",
