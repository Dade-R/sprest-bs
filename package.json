{
  "name": "gd-sprest-bs",
  "version": "5.0.9",
  "description": "SharePoint JavaScript, TypeScript and Web Components designed using the Bootstrap framework.",
  "main": "build/index.js",
  "typings": "@types/index.d.ts",
  "scripts": {
    "all": "npm run clean && npm run build && npm run prod && npm run typings",
    "build": "tsc && npm run build-sass && webpack --mode=development",
    "build-sass": "webpack --config webpack.sass.js",
    "clean": "node clean.js",
    "prod": "webpack --mode=production",
    "typings": "dts-bundle --configJson dts-bundle.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gunjandatta/sprest-bs.git"
  },
  "keywords": [
    "Bootstrap",
    "SharePoint",
    "SPFx"
  ],
  "author": "Gunjan Datta <me@dattabase.com> (https://dattabase.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gunjandatta/sprest-bs/issues"
  },
  "homepage": "https://dattabase.com/extras/bs",
  "dependencies": {
    "flatpickr": "^4.6.3",
    "gd-bs": "^3.0.8",
    "gd-sprest": "^5.8.6",
    "gd-sprest-def": "^0.7.8"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.6.0",
    "dts-bundle": "^0.7.3",
    "node-sass": "^4.14.1",
    "sass-loader": "^9.0.1",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  }
}
